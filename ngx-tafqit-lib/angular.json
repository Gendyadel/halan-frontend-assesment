{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-tafqit": {
      "projectType": "library",
      "root": "projects/ngx-tafqit",
      "sourceRoot": "projects/ngx-tafqit/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-tafqit/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-tafqit/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-tafqit/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-tafqit/src/test.ts",
            "tsConfig": "projects/ngx-tafqit/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-tafqit/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "e09a8f5c-c287-450f-b8c4-a130b2b01360"
  }
}
